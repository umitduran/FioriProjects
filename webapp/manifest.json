{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"ach": "CA",
		"resources": "resources.json",
		"id": "com.silverline.ticariurun",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZBPM_TU_SRV": {
				"uri": "/sap/opu/odata/sap/ZBPM_TU_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"ZBPM_GENEL_SRV": {
				"uri": "/sap/opu/odata/sap/ZBPM_GENEL_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZBPM_GENEL_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.silverline.ticariurun.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.silverline.ticariurun.i18n.i18n"
				}
			},
			"ecc": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZBPM_TU_SRV",
				"preload": true
			},
			"genel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZBPM_GENEL_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"targets": {
				"talepformu": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "TalepFormu"
				},
				"tedarikformu": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "TedarikFormu"
				}
			},
			"routes": [
				{
					"name": "talepformu",
					"pattern": "",
					"greedy": false,
					"target": [
						"talepformu"
					]
				},
				{
					"name": "tedarikformu",
					"pattern": "tedarikformu",
					"greedy": false,
					"target": [
						"tedarikformu"
					]
				}
			],
			"config": {
				"controlId": "app",
				"controlAggregation": "pages",
				"routerClass": "sap.m.routing.Router",
				"viewPath": "com.silverline.ticariurun.view"
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zbpm_ticariurun/webapp",
		"_version": "1.1.0"
	}
}