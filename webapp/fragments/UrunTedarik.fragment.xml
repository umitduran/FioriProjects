<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:u="sap.ui.unified"
	xmlns:core="sap.ui.core">
	<List
		items="{/TedarikCollection}"
		headerText="Bulunan Ürünler">
		<items>
			<ObjectListItem
				title="{TedarikciAdi}"
				type="Active"
				press="onListItemPress"
				number="{parts:[{path:'Fiyat'},{path:'HedefFiyatPB'}],
						type: 'sap.ui.model.type.Currency' }">
				<firstStatus>
					<ObjectStatus text="{UretimSuresi}"/>	
				</firstStatus>
			</ObjectListItem>
		</items>
	</List>
	</core:FragmentDefinition>